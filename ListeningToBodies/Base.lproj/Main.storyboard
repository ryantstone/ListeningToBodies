        <!--Breathing and Breathing Practices-->
        <scene sceneID="FJd-Ha-Qys">
            <objects>
                <viewController title="Breathing and Breathing Practices" id="1YR-GB-a7J" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="H2P-xB-fGK">
                        <rect key="frame" x="0.0" y="0.0" width="375" height="667"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" editable="NO" textAlignment="natural" translatesAutoresizingMaskIntoConstraints="NO" id="hM6-Jp-PA6">
                                <rect key="frame" x="0.0" y="64" width="375" height="603"/>
