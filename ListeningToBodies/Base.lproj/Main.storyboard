        <!--Listening to Bodies in Four Basic Moods-->
        <scene sceneID="FLK-HM-iFW">
            <objects>
                <viewController title="Listening to Bodies in Four Basic Moods" id="Jal-2j-3qC" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="fgw-S2-vfO">
                        <rect key="frame" x="0.0" y="0.0" width="375" height="667"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
